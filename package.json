{
  "name": "app.zetkin.org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "devserver": "next dev -p 80",
    "lint": "eslint src cypress && tsc --noEmit && echo 'Finished linting without errors!'",
    "tdd": "concurrently 'npm run devserver' 'npx cypress open'",
    "cypress-ci": "concurrently 'npm start' 'npx cypress run'",
    "watch": "chokidar 'src/**/*' -c 'npm run lint'",
    "start": "next start -p 80"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.7.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@react-spectrum/tabs": "^3.0.0-alpha.3",
    "next": "^10.0.3",
    "next-session": "^3.4.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "^5.13.2",
    "react-query": "^3.5.11",
    "zetkin": "^1.2.1"
  },
  "devDependencies": {
    "@cypress/react": "^4.16.3",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "@zeit/next-css": "^1.0.1",
    "babel-loader": "^8.2.2",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "cypress": "^6.3.0",
    "eslint": "^7.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-switch-statements": "^1.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^4.1.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2"
  }
}
